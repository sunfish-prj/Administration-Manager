@model SUNFISH.ViewModels.New.AccessPolicyVM

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

    <div>
        <h4>AccessPolicy</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>Access Policy Id</dt>
            <dd>
                @Html.DisplayFor(model => model.Id)
            </dd>
            <dt>Description</dt>
