@model SUNFISH.ViewModels.New.AccessPolicyVM

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>

    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>AccessPolicy</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>Access Policy Id</dt>
            <dd>
                @Html.DisplayFor(model => model.Id)
            </dd>
            <dt>Description</dt>
            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>
            <dt>Serving Tenant</dt>
            <dd>
                @Html.DisplayFor(model => model.ServTenantId)
            </dd>
            <dt>Serving Service</dt>
            <dd>
                @Html.DisplayFor(model => model.ServServiceId)
            </dd>
            <dt>Respective Tenant</dt>
            <dd>
                @Html.DisplayFor(model => model.RespTenantId)
            </dd>
            <dt>Respective Service</dt>
            <dd>
                @Html.DisplayFor(model => model.RespServiceId)
            </dd>
            <dt>Anonymized</dt>
            <dd>
                @Html.DisplayFor(model => model.IsAnonymized)
            </dd>
            <dt>Masked</dt>
            <dd>
                @Html.DisplayFor(model => model.IsMasked)
            </dd>
        </dl>

        <form asp-action="Delete">
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                <a asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>
</body>
</html>